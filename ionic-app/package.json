{
  "name": "ionic-app-base",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "tsc && vite build --mode production",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "test:performance": "tsx src/tests/performance.ts",
    "test:full": "tsx src/tests/fullAppTester.ts",
    "test:geolocation": "echo 'Ouvrir http://localhost:8100 et tester le composant GeolocationAudioTester'",
    "validate:final": "node validate-final.mjs",
    "lint": "eslint",
    "optimize": "tsx src/scripts/production.ts",
    "validate": "npm run lint && npm run build:prod",
    "deploy:prepare": "npm run validate && npm run optimize",
    "setup:tests": "node setup-tests.cjs",
    "test:web": "node test-web.cjs",
    "test:android": "node test-android.cjs",
    "test:ios": "node test-ios.cjs",
    "cap:sync": "npx cap sync",
    "cap:android": "npm run build && npx cap sync android && npx cap open android",
    "cap:ios": "npm run build && npx cap sync ios && npx cap open ios",
    "build:optimized": "npx vite build --config vite.config.optimized.ts",
    "analyze:performance": "node analyze-performance.cjs",
    "optimize:complete": "npm run build:optimized && npm run analyze:performance",
    "deploy": "node deploy.cjs",
    "deploy:android": "node deploy.cjs android",
    "deploy:ios": "node deploy.cjs ios",
    "demo": "node scripts/demo.cjs",
    "quick-start": "node scripts/dev-start.cjs",
    "report": "node scripts/final-report.cjs",
    "deploy:pwa": "node deploy.cjs pwa",
    "deploy:init": "node deploy.cjs --init"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/device": "^7.0.2",
    "@capacitor/filesystem": "^7.1.4",
    "@capacitor/geolocation": "^7.1.5",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/local-notifications": "^7.0.3",
    "@capacitor/network": "^7.0.2",
    "@ionic/core": "^8.3.0",
    "@ionic/react": "^8.3.0",
    "@ionic/react-router": "^8.3.0",
    "@ionic/storage-angular": "^4.0.0",
    "@reduxjs/toolkit": "^2.9.0",
    "@types/mapbox-gl": "^3.4.1",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.12.2",
    "firebase": "^12.2.1",
    "ionicons": "^7.4.0",
    "mapbox-gl": "^3.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4"
  },
  "devDependencies": {
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "cypress": "^13.5.0",
    "eslint": "^9.20.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.6.2",
    "terser": "^5.4.0",
    "tsx": "^4.20.5",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.24.0",
    "vite": "~5.2.0",
    "vitest": "^0.34.6"
  },
  "overrides": {
    "rollup": "4.44.0"
  }
}
